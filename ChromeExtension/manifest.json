{
  "name": "Example Gmail Extension",
  "description": "Example extension showing use of the Streak Gmail SDK",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": [
        "inboxsdk.js",
        "jquery-3.4.1.min.js",
        "arrive.min.js",
        "content.js"
      ],
      "css": [],
      "run_at": "document_end"
    }
  ],
  "permissions": ["https://mail.google.com/", "https://inbox.google.com/"],
  "manifest_version": 2
}
